  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Test",
      "program": "${workspaceFolder}/node_modules/.bin/jest",
      "args": [
        "--runInBand",
        "src/"
      ],
      "console": "internalConsole",
      "internalConsoleOptions": "neverOpen",
      "windows": {
        "program": "${workspaceFolder}/node_modules/jest/bin/jest",
      },
      "disableOptimisticBPs": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/samples/drone_delivery/provider_sample.js",
      "env": {
        "NODE_ENV": "development",
        "BLOCKCHAIN_TYPE": "ETH_LOCAL_TESTNET"
      }
    }
  ]
}
